#include <iostream>
#include <conio.h>
using namespace std;

int main()
{
    float n;

    cout << "Enter a number: ";
    cin >> n;
    cout << "Multiplication Table of: " << n << endl;
    
    for (float x = 1; x <= 10; ++x) 
	{
        cout << x << " * " << n << " = " << x * n << endl;
    }
    
    return 0;
}
