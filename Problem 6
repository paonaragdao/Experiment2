#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
    int n, num1=1, num2=2, num;
    cout << "The Next 20 Fibonacci Numbers [After 0 and 1]" << endl;
    
    
    for (int x = 0; x < 20; x++)
    {
    	cout << num1 << ", " << endl;
    	num = num1 + num2;
    	num1 = num2;
    	num2 = num;
	}
return 0;
}
